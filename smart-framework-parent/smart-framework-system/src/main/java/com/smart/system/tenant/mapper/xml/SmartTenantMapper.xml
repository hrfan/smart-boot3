        <result column="enable_ignore_case" property="enableIgnoreCase" jdbcType="VARCHAR"/>
        <result column="remark" property="remark" jdbcType="VARCHAR"/>
        <result column="is_corporate_tenant" property="isCorporateTenant" jdbcType="VARCHAR"/>
        <result column="main_menus" property="mainMenus" jdbcType="LONGVARCHAR"/>
        <result column="portlet_code" property="portletCode" jdbcType="VARCHAR"/>
    </resultMap>

    <!-- 基础字段 -->
    <sql id="Base_Column_List">
        id, tenant_code, tenant_name, operation_flag, create_by, create_time, create_user_name,
        update_by, update_time, update_user_name, sys_org_code, login_url, max_users, enable_division,
        enable_proxy, enable_ignore_case, remark, is_corporate_tenant, main_menus, portlet_code
